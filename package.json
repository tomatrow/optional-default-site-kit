{
    "name": "optional-default-site-kit",
    "version": "0.3.0",
    "scripts": {
        "dev": "svelte-kit dev",
        "build": "svelte-kit build",
        "package": "svelte-kit package",
        "preview": "svelte-kit preview",
        "prepare": "svelte-kit sync",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --check .",
        "format": "prettier --write .",
        "publish": "pnpm run lint && pnpm run package && pnpm publish ./package"
    },
    "devDependencies": {
        "@sveltejs/kit": "next",
        "autoprefixer": "^10.4.7",
        "cssnano": "^5.1.9",
        "lodash": "^4.17.21",
        "postcss": "^8.4.14",
        "postcss-load-config": "^3.1.4",
        "postcss-nested": "^5.0.6",
        "prettier": "^2.6.2",
        "prettier-plugin-svelte": "^2.7.0",
        "svelte": "^3.48.0",
        "svelte-check": "^2.7.1",
        "svelte-language-server": "^0.14.27",
        "svelte-preprocess": "^4.10.6",
        "svelte2tsx": "^0.5.10",
        "tailwindcss": "^2.2.19",
        "tslib": "^2.4.0",
        "typescript": "^4.6.4"
    },
    "type": "module",
    "homepage": "https://github.com/tomatrow/optional-default-site-kit#readme",
    "author": "AJ Caldwell <tomatrow@gmail.com>",
