{
    "name": "optional-default-site-kit",
    "version": "0.3.9",
    "scripts": {
        "dev": "vite dev",
        "build": "svelte-kit sync && svelte-package",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --check .",
        "format": "prettier --write .",
        "publish": "pnpm run lint && pnpm run build && pnpm publish ./package"
    },
    "devDependencies": {
        "@sveltejs/kit": "1.0.0-next.484",
        "@sveltejs/package": "1.0.0-next.3",
        "autoprefixer": "^10.4.11",
        "cssnano": "^5.1.13",
        "lodash": "^4.17.21",
        "postcss": "^8.4.16",
        "postcss-load-config": "^4.0.1",
        "prettier": "^2.7.1",
        "prettier-plugin-svelte": "^2.7.0",
        "svelte": "^3.50.1",
        "svelte-check": "^2.9.0",
        "svelte-language-server": "^0.14.35",
        "svelte-preprocess": "^4.10.7",
        "tailwindcss": "^3.1.8",
        "tslib": "^2.4.0",
        "typescript": "^4.8.3",
        "vite": "^3.1.2"
    },
    "type": "module",
    "homepage": "https://github.com/tomatrow/optional-default-site-kit#readme",
    "author": "AJ Caldwell <tomatrow@gmail.com>"
}
