{
    "name": "optional-default-site-kit",
    "version": "0.3.5",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "package": "svelte-kit package",
        "preview": "vite preview",
        "prepare": "svelte-kit sync",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --check .",
        "format": "prettier --write .",
        "publish": "pnpm run lint && pnpm run package && pnpm publish ./package"
    },
    "devDependencies": {
        "@sveltejs/kit": "next",
        "autoprefixer": "^10.4.7",
        "cssnano": "^5.1.9",
        "lodash": "^4.17.21",
        "postcss": "^8.4.14",
        "postcss-load-config": "^3.1.4",
        "prettier": "^2.6.2",
        "prettier-plugin-svelte": "^2.7.0",
        "svelte": "^3.49.0",
        "svelte-check": "^2.8.0",
        "svelte-language-server": "^0.14.29",
        "svelte-preprocess": "^4.10.7",
        "svelte2tsx": "^0.5.10",
        "tailwindcss": "^3.1.6",
        "tslib": "^2.4.0",
        "typescript": "^4.7.4",
        "vite": "^3.0.0"
    },
    "type": "module",
    "homepage": "https://github.com/tomatrow/optional-default-site-kit#readme",
    "author": "AJ Caldwell <tomatrow@gmail.com>"
}
